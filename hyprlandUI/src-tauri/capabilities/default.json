{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "hyprctl-clients",
          "cmd": "hyprctl",
          "args": [
            "clients"
          ]
        }
      ]
    }
  ]
}